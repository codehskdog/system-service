generator client {
    provider = "prisma-client-js"
}

generator prismaClassGenerator {
    provider   = "prisma-class-generator"
    output     = "../libs/prisma/src/class"
    dryRun     = false
    useSwagger = false
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id       String  @id @unique @default(uuid())
    email    String? @unique
    name     String?
    username String? @unique
    password String?
}
